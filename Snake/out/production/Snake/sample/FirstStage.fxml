<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.FirstStage">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true">
         <image>
            <Image url="@../background.jpg" />
         </image>
      </ImageView>
      <Button fx:id="btnPlay" layoutX="260.0" layoutY="288.0" mnemonicParsing="false" onAction="#Play" prefHeight="45.0" prefWidth="81.0" text="Tìm trận">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="btnShop" layoutX="36.0" layoutY="333.0" mnemonicParsing="false" text="Cửa hàng" />
      <Button fx:id="btnLogout" layoutX="502.0" layoutY="333.0" mnemonicParsing="false" onAction="#Logout" text="Đăng xuất" />
      <Label layoutX="14.0" layoutY="14.0" text="Player:" textFill="#fffbfb">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="Username" layoutX="65.0" layoutY="14.0" text="Label" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
